<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms">
  <h:head>
    <h:title>Rapid Diagnostic Toolkit Demo</h:title>
    <model>
      <itext>
        <translation lang="en">
          <text id="/rdtoolkit_capture/data/patient_id:label">
            <value>Patient ID</value>
          </text>
          <text id="/rdtoolkit_capture/data/patient_name:label">
            <value>Patient name</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_image:label">
            <value>Image</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_results_description:label">
            <value>Results</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_session_id:label">
            <value>Session ID</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_state:label">
            <value>Status</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_time_read:label">
            <value>Taken on</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_time_resolved:label">
            <value>Results were available on</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_time_started:label">
            <value>Started on</value>
          </text>
          <text id="/rdtoolkit_capture/patient_uuid:jr:constraintMsg">
            <value>-</value>
          </text>
        </translation>
        <translation lang="fr">
          <text id="/rdtoolkit_capture/_patient_id:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/_patient_name:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/_rdtoolkit_session_id:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/patient_id:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/patient_name:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_image:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_results:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_results_description:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_session_id:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_state:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_time_read:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_time_resolved:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data/rdtoolkit_time_started:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/data:label">
            <value>-</value>
          </text>
          <text id="/rdtoolkit_capture/patient_uuid:jr:constraintMsg">
            <value>../data/contact/_id</value>
          </text>
          <text id="/rdtoolkit_capture/patient_uuid:label">
            <value>-</value>
          </text>
        </translation>
      </itext>
      <instance>
        <rdtoolkit_capture id="rdtoolkit_capture" prefix="J1!rdtoolkit_capture!" delimiter="#" version="2021-05-10 01:22:00">
          <patient_uuid tag="hidden"/>
          <_patient_id tag="hidden"/>
          <_patient_name tag="hidden"/>
          <_rdtoolkit_session_id tag="hidden"/>
          <data>
            <patient_name/>
            <patient_id/>
            <rdtoolkit_session_id tag="hidden"/>
            <rdtoolkit_state/>
            <rdtoolkit_time_started/>
            <rdtoolkit_time_resolved tag="hidden"/>
            <rdtoolkit_results tag="hidden"/>
            <rdtoolkit_results_description/>
            <rdtoolkit_time_read/>
            <rdtoolkit_image type="binary"/>
          </data>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </rdtoolkit_capture>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/rdtoolkit_capture/patient_uuid" type="string" jr:constraintMsg="jr:itext('/rdtoolkit_capture/patient_uuid:jr:constraintMsg')"/>
      <bind nodeset="/rdtoolkit_capture/_patient_id" type="string"/>
      <bind nodeset="/rdtoolkit_capture/_patient_name" type="string"/>
      <bind nodeset="/rdtoolkit_capture/_rdtoolkit_session_id" type="string"/>
      <bind nodeset="/rdtoolkit_capture/data/patient_name" type="string" calculate=" /rdtoolkit_capture/_patient_name " readonly="true()"/>
      <bind nodeset="/rdtoolkit_capture/data/patient_id" type="string" calculate=" /rdtoolkit_capture/_patient_id " required="true()" readonly="true()"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_session_id" type="string" calculate=" /rdtoolkit_capture/_rdtoolkit_session_id " required="true()"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_state" type="string"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_time_started" type="string"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_time_resolved" type="string"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_results" type="string"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_results_description" type="string"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_time_read" type="string"/>
      <bind nodeset="/rdtoolkit_capture/data/rdtoolkit_image" type="string"/>
      <bind nodeset="/rdtoolkit_capture/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list rdtoolkit_capture" ref="/rdtoolkit_capture/data">
      <input ref="/rdtoolkit_capture/data/patient_name">
        <label ref="jr:itext('/rdtoolkit_capture/data/patient_name:label')"/>
      </input>
      <input appearance="rdtoolkit_action_btn" ref="/rdtoolkit_capture/data/patient_id">
        <label ref="jr:itext('/rdtoolkit_capture/data/patient_id:label')"/>
      </input>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_session_id">
        <label ref="jr:itext('/rdtoolkit_capture/data/rdtoolkit_session_id:label')"/>
      </input>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_state">
        <label ref="jr:itext('/rdtoolkit_capture/data/rdtoolkit_state:label')"/>
      </input>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_time_started">
        <label ref="jr:itext('/rdtoolkit_capture/data/rdtoolkit_time_started:label')"/>
      </input>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_time_resolved">
        <label ref="jr:itext('/rdtoolkit_capture/data/rdtoolkit_time_resolved:label')"/>
      </input>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_results"/>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_results_description">
        <label ref="jr:itext('/rdtoolkit_capture/data/rdtoolkit_results_description:label')"/>
      </input>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_time_read">
        <label ref="jr:itext('/rdtoolkit_capture/data/rdtoolkit_time_read:label')"/>
      </input>
      <input appearance="hidden" ref="/rdtoolkit_capture/data/rdtoolkit_image">
        <label ref="jr:itext('/rdtoolkit_capture/data/rdtoolkit_image:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
